{
  "kind": "collectionType",
  "collectionName": "seats",
  "displayName": "Seat",
  "info": {
    "singularName": "seat",
    "pluralName": "seats",
    "displayName": "seat"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "seatNumber": {
      "type": "string"
    },
    "row": {
      "type": "integer"
    },
    "column": {
      "type": "integer"
    },
    "price": {
      "type": "integer"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "XL",
        "LOWER",
        "UPPER"
      ]
    },
    "venue": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::venue.venue",
      "inversedBy": "seats"
    },
    "seatid": {
      "type": "uid"
    },
    "is_synced": {
      "type": "boolean",
      "default": false
    }
  }
}
