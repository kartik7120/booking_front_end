{
  "kind": "collectionType",
  "collectionName": "movietimeslots",
  "displayname": "Movie Time Slot",
  "info": {
    "singularName": "movietimeslot",
    "pluralName": "movietimeslots",
    "displayName": "movietimeslot"
  },
  "options": {
    "comment": "",
    "draftAndPublish": true
  },
  "attributes": {
    "starttime": {
      "type": "datetime",
      "required": true
    },
    "endtime": {
      "type": "datetime",
      "required": true
    },
    "duration": {
      "type": "integer",
      "required": true
    },
    "movieformat": {
      "type": "enumeration",
      "enum": [
        "TWO_D",
        "THREE_D",
        "IMAX",
        "FOUR_D"
      ],
      "required": true
    },
    "movie": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::movie.movie",
      "inversedBy": "timeslots",
      "required": true
    },
    "venue": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::venue.venue",
      "inversedBy": "timeslots",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "is_synced": {
      "type": "boolean",
      "default": false
    },
    "starpi_movie_time_slot_uid": {
      "type": "string",
      "unique": true
    },
    "movie_time_slot_id": {
      "type": "integer",
      "unique": true
    }
  }
}